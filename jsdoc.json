{
    "tags": {
      "allowUnknownTags": true
    },
    "source": {
      "include": ["./src"],
      "includePattern": ".+\\.js(doc|x)?$"
    },
    "plugins": ["plugins/markdown"],    
    "opts": {
        "encoding": "utf8",
        "destination": "docs/",
        "recurse": true,
        "verbose": true,
        "readme": "./README.md"
    },
    "templates": {
        "better-docs": {
          "name": "modal_hrnet doc"
        }
      }
  }